{
	"$schema" : "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"version" : 2,
	"console_title_template" : "{{ .Shell }}{{ if .Root }} (elevated){{end}} in {{ .PWD }}",
	"final_space" : true,
	"palette" :
	{
		"black" : "#262B44",
		"white" : "#E0DEF4",
		"grey" : "#303030",
		"blue" : "#0077C2",
		"green" : "#097a44",
		"mint" : "#00897b",
		"red" : "#99051d",
		"orange" : "#F07623",
		"yellow" : "#F3AE35"
	},
	"blocks" :
	[
		{
			"alignment" : "left",
			"segments" :
			[
				{
					"type" : "session",
					"style" : "diamond",
					"leading_diamond" : "\ue0b6",
					"trailing_diamond" : "\ue0b0",
					"background" : "p:white",
					"background_templates" :
					[
						"{{ if .Root }}p:yellow{{ end }}"
					],
					"foreground" : "p:black",
					"template" : "{{ if .Root }}\uf071{{ end }} {{ if .SSHSession }}\uf817 {{ .UserName }}@{{ .HostName }}{{ else }}{{ .UserName }}{{ end }} "
				},
				{
					"type" : "path",
					"style" : "powerline",
					"powerline_symbol" : "\ue0b0",
					"background" : "p:blue",
					"foreground" : "p:white",
					"properties" :
					{
						"home_icon" : "\uf7db",
						"style" : "folder"
					},
					"template" : " \uf74a {{ path .Path .Location }} "
				},
				{
					"type" : "git",
					"style" : "powerline",
					"powerline_symbol" : "\ue0b0",
					"background" : "p:mint",
					"background_templates" :
					[
						"{{ if or (.Working.Changed) (.Staging.Changed) }}p:grey{{ end }}",
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
						"{{ if gt .Ahead 0 }}p:orange{{ end }}",
						"{{ if gt .Behind 0 }}p:orange{{ end }}"
					],
					"foreground" : "p:white",
					"properties" :
					{
						"fetch_status" : true,
						"fetch_stash_count" : true,
						"fetch_upstream_icon" : true,
						"branch_icon" : "\ue725 "
					},
					"template" : " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} "
				},
				{
					"type" : "executiontime",
					"style" : "diamond",
					"leading_diamond": "<transparent,background>\ue0b0</>",
					"trailing_diamond": "\ue0b4",
					"background" : "p:green",
					"background_templates" :
					[
						"{{ if gt .Code 0 }}p:red{{ end }}"
					],
					"foreground" : "p:white",
					"properties" :
					{
						"threshold" : 0,
						"style" : "austin"
					},
					"template" : " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} {{ .FormattedMs }} "
				}
			],
			"type": "prompt"
		},
		{
			"segments" :
			[
				{
					"type" : "node",
					"style" : "diamond",
					"leading_diamond": " \ue0b6",
					"trailing_diamond": "\ue0b4 ",
					"background" : "p:grey",
					"foreground" : "p:green",
					"properties" :
					{
						"display_mode" : "files",
						"home_enabled" : true
					},
					"template" : "\uf898 <p:white>{{ .Full }}</>"
				},
				{
					"type" : "dotnet",
					"style" : "diamond",
					"leading_diamond": " \ue0b6",
					"trailing_diamond": "\ue0b4 ",
					"background" : "p:grey",
					"foreground" : "p:blue",
					"properties" :
					{
						"display_mode" : "files",
						"home_enabled" : true
					},
					"template" : "\uf81a <p:white>{{ .Full }}</>"
				},
				{
					"type" : "aws",
					"style" : "diamond",
					"leading_diamond": " \ue0b6",
					"trailing_diamond": "\ue0b4 ",
					"background" : "p:grey",
					"foreground" : "p:yellow",
					"template" : "\uf270 {{ .Profile }}{{ if .Region }} ({{ .Region }}){{ end }}"
				}
			],
			"type" : "rprompt"
		}
	],

	"secondary_prompt" :
	{
		"background" : "transparent",
		"foreground" : "p:white",
		"template" : "> "
	},

	"transient_prompt" :
	{
		"background" : "transparent",
		"foreground" : "p:white",
		"template" : "> "
	},

	"tooltips" : []
}
